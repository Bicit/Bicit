<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Autor</th>
      <th>Privacidad</th>
      <th>Fecha publicacion</th>
      <th>Fecha evento</th>
      <th>Distancia</th>
      <th>Duracion</th>
      <th>Asistentes</th>
      <th>Tal ves</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.nombre %></td>
        <td><%= event.autor %></td>
        <td><%= event.privacidad %></td>
        <td><%= event.fecha_publicacion %></td>
        <td><%= event.fecha_evento %></td>
        <td><%= event.distancia %></td>
        <td><%= event.duracion %></td>
        <td><%= event.asistentes %></td>
        <td><%= event.tal_ves %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
